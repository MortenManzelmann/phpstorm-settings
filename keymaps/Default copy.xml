<keymap version="1" name="Default copy" parent="$default">
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="alt c" />
  </action>
  <action id="Exit">
    <keyboard-shortcut first-keystroke="alt x" second-keystroke="alt x" />
  </action>
  <action id="RecentProjectListGroup">
    <keyboard-shortcut first-keystroke="shift ctrl o" />
  </action>
</keymap>